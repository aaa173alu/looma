<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Infrastructure" namespace="ApplicationCore.Domain.EN">
  <class name="Sesion" table="Sesion">
    <id name="Id" column="Id" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="FechaInicio" column="FechaInicio" />
    <property name="FechaFin" column="FechaFin" />
    <property name="Activa" column="Activa" />
    <property name="UsuarioId" column="UsuarioId" />
  </class>
</hibernate-mapping>
