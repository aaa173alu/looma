<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ApplicationCore" namespace="ApplicationCore.Domain.EN">
  <class name="ItemPedido" table="ItemPedido">
    <id name="Id" column="Id" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="ProductoId" column="ProductoId" />
    <property name="Cantidad" column="Cantidad" />
    <property name="Talla" column="Talla" />
  </class>
</hibernate-mapping>
