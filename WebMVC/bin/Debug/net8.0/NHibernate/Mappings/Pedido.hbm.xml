<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ApplicationCore" namespace="ApplicationCore.Domain.EN">
  <class name="Pedido" table="Pedido">
    <id name="Id" column="Id" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="Fecha" column="Fecha" />
    <property name="Total" column="Total" />
    <property name="DireccionEnvio" column="DireccionEnvio" />
    <property name="Estado" column="Estado" />
    <property name="UsuarioId" column="UsuarioId" />
    <bag name="Items" table="ItemPedido" cascade="all" inverse="false" lazy="false">
      <key column="PedidoId" />
      <one-to-many class="ItemPedido" />
    </bag>
  </class>
</hibernate-mapping>
